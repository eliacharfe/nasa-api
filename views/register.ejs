<%- include('includes/head.ejs') %>

<body class="bg-secondary" >
<div class="container bg-dark myborder ">

    <div class="row">
        <div class="col-xl-4 col-md-3 "></div>

        <div class="col-xl-4 col-md-6">
            <div class="card border myborder border-info border-3  bg-light mt-4 mb-4">
                <div class="card-body">
                    <p class="alert alert-danger <% if (msg === "") { %> d-none <% } %>"><strong><%= msg %> </strong></p>

                    <nav class="navbar navbar-expand-lg navbar-light backInfo border border-primary myborder py-0 mb-2">
                        <div class="container-fluid mb-2 ">
                            <h3 class=" navbar-text text-white  rounded mb-0 "><%= title %></h3>
                        </div>
                    </nav>
                    <img src="https://c.tenor.com/WbLLhduoplUAAAAC/capoo-bugcat.gif"
                         class="card-img-top myborder imgGif <% if (msg !== '') { %> d-none <% } %>" alt="..." >

                    <!-- FORM-->
                    <form action="/password" method="post" id="ajaxValidationRegisterPost"
                          autocomplete="on" class="ml-3 mb-1 mr-3">

                        <div class="mb-2"><!--Email-->
                            <label for="email" class="form-label">Email address</label>
                            <span class="star text-danger">*</span>
                            <span> <img src="data:image/png;base64,"
                                        class="card-img-top myborder imgLogo"   alt="..." ></span>
                            <input type="email" name="email" class="form-control myborder myinput" id="email"
                                   placeholder="Enter your email here" autocomplete="true" required>
                            <!--      pattern="^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$"-->
                            <!--    title="format example: dani22@gmail.com
                                     any lowercase/uppercase/numbers + '@" + any lower/uppercase + '.' + 2 or 3 any lower/uppercase-->
                            <div class="text-danger errormessage"></div>
                        </div>

                        <div class="mb-2"><!--First name-->
                            <label for="firstName" class="form-label">First name</label>
                            <span class="star text-danger">*</span>
                            <span> <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcThO6Z1SVPB6ZzVhAGvyX9PUfv-v9jaSKRsrQKeLuSS76I9_SMQiKomVPNKj1s-alJV3Zc&usqp=CAU"
                                        class="card-img-top myborder imgLogo "  alt="..." ></span>
                            <input type="text" name="firstName" class="form-control myborder myinput" id="firstName"
                                   placeholder="Enter your first name" autocomplete="on"
                                   pattern="^[A-Za-z.\s_-]+$"
                                   title="Only uppercase or lowercase letters" required>
                            <div class="text-danger errormessage"></div>
                        </div>

                        <div class="mb-2"><!--Last name-->
                            <label for="lastName" class="form-label">Last name</label>
                            <span class="star text-danger">*</span>
                            <span> <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcThO6Z1SVPB6ZzVhAGvyX9PUfv-v9jaSKRsrQKeLuSS76I9_SMQiKomVPNKj1s-alJV3Zc&usqp=CAU"
                                        class="card-img-top myborder imgLogo behind"  alt="..." ></span>
                            <input type="text" name="lastName" class="form-control myborder myinput" id="lastName"
                                   placeholder="Enter your last name" autocomplete="on"
                                   pattern="^[A-Za-z.\s_-]+$"
                                   title="Only uppercase or lowercase letters" required>
                            <div class="text-danger errormessage"></div>
                        </div>

                        <input type="submit" name="submitReg" id="submitReg" value="Next"
                               class="btn btn-block  border border-primary  mybtn backInfo btn-active-bg-shade-amount mr-3  w-100"
                               data-toggle="tooltip" data-placement="top"
                               title="Note: you'll have only 1 minute to register a password!">
                        <div class="mt-2 mb-0 ">
                            <p class="text-center"><i>Already have an account? </i><a href="/">Login here</a></p>
                        </div>
                    </form>

                </div>
                <img src="https://i.gifer.com/TEcu.gif" class="card-img-top myborder" alt="...">
            </div>
        </div>

        <div class="col-xl-4 col-md-3 "></div>
    </div>
</div>

<%- include('includes/end.ejs') %>